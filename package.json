{
  "name": "gaapjan-npm-package-demo",
  "version": "0.0.7",
  "description": "a npm package",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "co:login": "aws codeartifact login --tool npm --repository gaapjan-packages --domain gaapjan --domain-owner 440900076177 --profile default",
    "test": "jest --config jestconfig.json",
    "prepare": "tsc",
    "prepublish": "npm run co:login",
    "build:Dev": "rm -rf dist/js && npm run build",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "dev": "rollup -c -w",
    "build": "rollup -c && tsc",
    "build:watch": "rollup -cw"
  },
